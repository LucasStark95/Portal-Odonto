@model Model.Models.Usuario
<h1>Perfil</h1>
<div>
    <div class="row">
        <div class="col-md-3">
        </div>
        <div class="col-md-9">
            <fieldset>
                <legend>Dados do Usuário</legend>
                <div class="form-horizontal">
                    <div class="container text-center">
                        <div class="row thumbnail">
                            <hr />
                            <div class="form-group">
                                <div>
                                    @Html.LabelFor(model => model.NomeUsuario, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-3">

                                        @Html.DisplayFor(model => model.NomeUsuario, new { htmlAttributes = new { @class = "control-label col-md-2" } })
                                    </div>

                                    @Html.LabelFor(model => model.NomeMae, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-3">

                                        @Html.DisplayFor(model => model.NomeMae, new { htmlAttributes = new { @class = "control-label col-md-2" } })
                                    </div>
                                    <div class="col-md-2"> </div>
                                </div>
                            </div>

                            <div class="form-group">
                                @Html.LabelFor(model => model.EmailUsuario, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-3">

                                    @Html.DisplayFor(model => model.EmailUsuario, new { htmlAttributes = new { @class = "control-label col-md-2" } })
                                </div>
                                @Html.LabelFor(model => model.DataNascimento, htmlAttributes: new { @class = "control-labeul col-md-2" })
                                <div class="col-md-3">
                                    @Model.DataNascimento.ToString("dd/MM/yyyy")
                                </div>
                                <div class="col-md-4"> </div>
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(model => model.TipoUsuario, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-3">
                                    @Html.DisplayFor(model => model.TipoUsuario, new { htmlAttributes = new { @class = "control-label col-md-2" } })
                                </div>
                                @Html.LabelFor(model => model.MatriculaUsuario, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-3">
                                    @Html.DisplayFor(model => model.MatriculaUsuario, new { htmlAttributes = new { @class = "control-label col-md-2" } })
                                </div>
                                <div class="col-md-4"> </div>
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(model => model.Endereco, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-3">
                                    @Html.DisplayFor(model => model.Endereco, new { htmlAttributes = new { @class = "control-label col-md-2" } })
                                </div>
                                @Html.LabelFor(model => model.Bairro, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-3">
                                    @Html.DisplayFor(model => model.Bairro, new { htmlAttributes = new { @class = "control-label col-md-2" } })
                                </div>
                                <div class="col-md-4"> </div>
                            </div>
                            <div class="form-group">
                                @Html.LabelFor(model => model.Cep, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-3">
                                    @Html.DisplayFor(model => model.Cep, new { htmlAttributes = new { @class = "control-label col-md-2" } })
                                </div>
                                @Html.LabelFor(model => model.Cidade, htmlAttributes: new { @class = "control-label col-md-2" })
                                <div class="col-md-3">
                                    @Html.DisplayFor(model => model.Cidade, new { htmlAttributes = new { @class = "control-label col-md-2" } })
                                </div>
                                <div class="col-md-4"> </div>
                            </div>
                            <br />
                            <p>
                                @Html.ActionLink("Editar", "Edit", new { id = Model.IdUsuario }, new { @class = "btn btn-default" }) |
                                @Html.ActionLink("Voltar", "Index", "", new { @class = "btn btn-default" })
                            </p>
                        </div>
                    </div>
                </div>
            </fieldset>
        </div>
    </div>
</div>